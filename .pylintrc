[MASTER]
extension-pkg-allow-list=hashlib

[MESSAGES CONTROL]
disable=
    C0103,  # invalid-name
    C0301,  # line-too-long
    R0903,  # too-few-public-methods
    R0913,  # too-many-arguments
    R0914,  # too-many-locals

[BASIC]
good-names=i,j,k,ex,Run,_

[DESIGN]
max-args=6
max-locals=15

[FORMAT]
max-line-length=100